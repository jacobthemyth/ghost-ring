include /mixins/spacify

mixin mkLinks(index, thing)
  if typeof(thing) === 'string'
    li
      a(href=thing)
        +spacify(index)
  else if typeof(thing) === 'object'
    ul
      +mkLinks(index, thing['index.html'])
      - delete thing['index.html']
      each subthing, subindex in thing
        +mkLinks(subindex, subthing)
